{
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
        "max-len": [2, 180],
        "quotes": [
            "error",
            "double"
        ],
        "indent": ["error", 2, {
            "SwitchCase": 1
        }]
    },
    "extends": ["prettier"]
}