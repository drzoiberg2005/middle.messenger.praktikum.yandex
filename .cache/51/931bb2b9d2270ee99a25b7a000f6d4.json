  list-style: none;\n    margin: 0;\n    padding: 0;\n}\n\n.list__item {\n    width: 100%;\n    height: 8rem;\n    border-bottom: 1px solid #2196F3;\n    padding: 1rem;\n    box-sizing: border-box;\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    justify-content: space-between;\n    overflow: hidden;\n\n    &.active {\n        border-left: 1rem solid #2196F3;\n        background: #ffffff;\n    }\n}\n\n\n.list__item-left-text {\n    display: flex;\n    flex-direction: column;\n    align-items: flex-start;\n    justify-content: space-between;\n}\n\n.list__item-left-avatar {\n    width: 30%;\n    height: 5rem;\n    fill: #cccccc;\n    margin-right: 2rem;\n}\n\n.list__item-left {\n    width: 20vw;\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    justify-content: space-between;\n}\n\n.list__item-right {\n    display: flex;\n    flex-direction: column;\n    align-items: flex-end;\n    justify-content: space-between;\n}\n\n.list__item-left-text-contact {\n    font-size: 2.4rem;\n    font-weight: 300\n}\n\n.list__item-left-text-message {\n    width: 15vw;\n    margin-top: 0.5rem;\n    font-size: 1.8rem;\n    font-weight: 300;\n    white-space: nowrap;\n    overflow: hidden;\n    text-overflow: ellipsis;\n}\n\n.list__item-right-time {\n    font-size: 1.4rem;\n    font-weight: 300\n}\n\n.list__item-right-counter {\n    margin-top: 1rem;\n    color: #ffffff;\n    background-color: #2196F3;\n    width: 2rem;\n    height: 2rem;\n    border-radius: 50%;\n    font-size: 1.4rem;\n    font-weight: 500;\n    padding-top: 0.2rem;\n    text-align: center;\n    box-sizing: border-box;\n}\n\n.dialog {\n    width: 60%;\n    box-sizing: border-box;\n}\n\n.messages {\n    margin: 0;\n    padding: 2rem;\n    height: 80vh;\n    display: flex;\n    flex-direction: column-reverse;\n    justify-content: flex-start;\n    overflow-y: auto;\n    box-sizing: border-box;\n    touch-action: none;\n    -ms-touch-action: none;\n    -webkit-overflow-scrolling: touch;\n    list-style: none\n}\n\n.messages__item {\n    display: flex;\n    flex-direction: row;\n    justify-content: flex-start;\n    box-sizing: border-box;\n    &.__send {\n        justify-content: flex-end;\n    }\n}\n\n.messages__item-block {\n    background: #00000010;\n    padding: 1rem;\n    max-width: 60%;\n    margin: 1rem 0;\n    border-radius: 0 10px 10px 0;\n    border-left: 0.5rem solid #69F0AE;\n    box-sizing: border-box;\n\n    &.__send {\n        border-radius: 10px 0 0 10px;\n        border-left: none;\n        border-right: 0.5rem solid #2196F3;\n    }\n}\n\n.message__item-text {\n    font-size: 1.6rem;\n}\n\n.shipment {\n    width: 100%;\n    height: 10vh;\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    justify-content: space-around;\n    padding: 1rem;\n    box-sizing: border-box;\n    border-top: 1px solid #2196F3;\n}\n\n.shipment__icon {\n    width: 3.5rem;\n    height: 3.5rem;\n    fill: #2196F3;\n}\n\n.shipment__input {\n    width: 60%;\n    border: 1px solid #2196F3;\n    border-radius: 10px;\n    padding: 0.5rem 1rem;\n    margin: 0 1rem;\n}\n\n.warning {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    font-size: 2rem;\n    height: 100%;\n}"},"lineCount":null}},"error":null,"hash":"8098b02914afc73f619292c332a1ddad","cacheData":{"env":{}}}