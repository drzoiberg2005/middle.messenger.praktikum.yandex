direction: row;\n    box-sizing: border-box;\n\n    @media only screen and (max-width: 1024px) {\n        height: 80vh;\n    }\n}\n\n.dialogs {\n    width: 70%;\n    height: 100%;\n    border-right: 1px solid $border-color;\n    background: #E3F2FD;\n    box-sizing: border-box;\n}\n\n.find {\n    height: 10%;\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n    border-bottom: 1px solid $border-color;\n    box-sizing: border-box;\n    padding: 1rem;\n}\n\n.find__input {\n    width: 75%;\n    border: 1px solid $border-color;\n    border-radius: 10px;\n    padding: 0.5rem 1rem;\n}\n\n.list {\n    height: 90%;\n    overflow-y: auto;\n    box-sizing: border-box;\n    list-style: none;\n    margin: 0;\n    padding: 0;\n}\n\n.list__item {\n    width: 100%;\n    height: 10%;\n    border-bottom: 1px solid $border-color;\n    padding: 1rem;\n    box-sizing: border-box;\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    justify-content: space-between;\n    overflow: hidden;\n\n    &.__active {\n        border-left: 1rem solid $chat-pointer;\n        background: $main-background;\n    }\n}\n\n\n.list__item-left-text {\n    display: flex;\n    flex-direction: column;\n    align-items: flex-start;\n    justify-content: space-between;\n}\n\n.list__item-left-avatar {\n    margin-right: 1rem;\n}\n\n.list__item-left {\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    justify-content: space-between;\n}\n\n.list__item-right {\n    display: flex;\n    flex-direction: column;\n    align-items: flex-end;\n    justify-content: space-between;\n}\n\n.list__item-left-text-contact {\n    font-size: 1.8rem;\n    font-weight: 300\n}\n\n.list__item-left-text-message {\n    width: 50%;\n    margin-top: 0.5rem;\n    font-size: 1.8rem;\n    font-weight: 300;\n    white-space: nowrap;\n    overflow: hidden;\n    text-overflow: ellipsis;\n}\n\n.list__item-right-time {\n    font-size: 1.4rem;\n    font-weight: 300\n}\n\n.list__item-right-counter {\n    margin-top: 1rem;\n    color: $unread-counter-text;\n    background-color: $unread-counter-background;\n    width: 2rem;\n    height: 2rem;\n    border-radius: 50%;\n    font-size: 1.4rem;\n    font-weight: 500;\n    padding-top: 0.2rem;\n    text-align: center;\n    box-sizing: border-box;\n}\n\n.dialog {\n    height: 100%;\n    width: 100%;\n    box-sizing: border-box;\n}\n\n.messages {\n    margin: 0;\n    padding: 2rem;\n    height: 90%;\n    display: flex;\n    flex-direction: column-reverse;\n    justify-content: flex-start;\n    overflow-y: auto;\n    box-sizing: border-box;\n    list-style: none\n}\n\n.messages__item {\n    display: flex;\n    flex-direction: row;\n    justify-content: flex-start;\n    box-sizing: border-box;\n\n    &.__send {\n        justify-content: flex-end;\n    }\n}\n\n.messages__item-block {\n    background: $light3;\n    padding: 1rem;\n    max-width: 60%;\n    margin: 1rem 0;\n    border-radius: 0 10px 10px 0;\n    border-left: 0.5rem solid $message-pointer-left;\n    box-sizing: border-box;\n\n    &.__send {\n        border-radius: 10px 0 0 10px;\n        border-left: none;\n        border-right: 0.5rem solid $message-pointer-right;\n    }\n}\n\n.message__item-text {\n    font-size: 1.6rem;\n}\n\n.shipment {\n    height: 10%;\n    width: 100%;\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    justify-content: space-around;\n    padding: 1rem;\n    box-sizing: border-box;\n    border-top: 1px solid $border-color;\n}\n\n.icon {\n    width: 3.5rem;\n    height: 3.5rem;\n    color: $icons-color;\n}\n\n.shipment__input {\n    width: 60%;\n    border: 1px solid $border-color;\n    border-radius: 10px;\n    padding: 0.5rem 1rem;\n    margin: 0 1rem;\n}\n\n.warning {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    font-size: 2rem;\n    height: 100%;\n}","../static/_variables.scss":"$light1: #ffffff;\n$light2: #e3f2fd;\n$light3: #eee;\n$dark1: 