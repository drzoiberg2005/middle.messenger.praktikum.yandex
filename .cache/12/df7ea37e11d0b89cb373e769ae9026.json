s","original":{"line":33,"column":11}},{"generated":{"line":40,"column":15},"source":"../src/utils/generateavatar.js","original":{"line":33,"column":4}},{"generated":{"line":41,"column":0},"source":"../src/utils/generateavatar.js","original":{"line":34,"column":1}}],"sources":{"../src/utils/generateavatar.js":"export default generateavatar = (object, size = 10, user = true, color = '#ffffff', background = '#000000') => {\n    const text = (user ? object.first_name[0] + object.second_name[0] : object.title[0]).toUpperCase()\n\n    const content = () => {\n        if (object.avatar) {\n            const image = document.createElement('img')\n            image.setAttribute('src', object.avatar)\n            image.style.borderRadius = '50%'\n            return image\n        } else {\n            const canvas = document.createElement('canvas')\n            canvas.width =\n                canvas.height = 10 * size\n            const context = canvas.getContext('2d')\n\n            context.fillStyle = background\n            context.fillRect(0, 0, canvas.width, canvas.height)\n\n            context.font = `${size/3}rem Montserrat-regular`\n            context.fillStyle = color\n            context.textAlign = 'center'\n            context.textBaseline = 'middle'\n            context.fillText(text, canvas.width / 2, canvas.height / 2)\n            return canvas\n        }\n    }\n\n    const avatar = content()\n    avatar.style.borderRadius = '50%'\n    avatar.style.height = `${size}rem`\n    avatar.style.width = `${size}rem`\n\n    return avatar\n}"},"lineCount":null}},"error":null,"hash":"63c5900205447a93f0643dfaa3966d15","cacheData":{"env":{}}}